<idea-plugin>
    <id>io.github.art.idea</id>
    <name>ART</name>
    <vendor email="anton.sh.local@gmail.com" url="https://github.com/art-community">art-community</vendor>

    <description>
        ART Plugin. ART Generated code completions and other.
    </description>

    <extensions defaultExtensionNs="com.intellij">
        <completion.contributor language="JAVA" implementationClass="io.art.idea.plugin.completion.ModelCompletion"/>
        <lang.psiAugmentProvider implementation="io.art.idea.plugin.augment.GeneratedAugmentProvider"/>
    </extensions>

</idea-plugin>
